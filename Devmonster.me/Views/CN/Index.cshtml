        function refilter() {
            $(".game-item").hide();
            var coins = $("#my_coins").val();
            var showmarked = 0;

            if (coins == 0) coins = 10000;
            if ($("#Marked").prop('checked')) showmarked = 1;
            


            if ($("#DS").prop('checked')) {
                $("li[data-system='3ds']").filter(function () {
                    if (showmarked == 1)
                        return $(this).attr("data-cost") <= parseInt(coins) && $(this).attr("data-state") == 'marked';
                    else
                        return $(this).attr("data-cost") <= parseInt(coins);
                }).show();
            }
            if ($("#Wii").prop('checked')) {
                $("li[data-system='wii']").filter(function () {
                    if (showmarked == 1)
                        return $(this).attr("data-cost") <= parseInt(coins) && $(this).attr("data-state") == 'marked';
                    else
                        return $(this).attr("data-cost") <= parseInt(coins);
                }).show();
            }
            if ($("#WiiU").prop('checked')) {
                $("li[data-system='wiiu']").filter(function () {
                    if (showmarked == 1)
                        return $(this).attr("data-cost") <= parseInt(coins) && $(this).attr("data-state") == 'marked';
                    else
                        return $(this).attr("data-cost") <= parseInt(coins);
                }).show();
            }

            if ($("#Physical").prop('checked')) {
                $("li[data-system='physical']").filter(function () {
                    if (showmarked == 1)
                        return $(this).attr("data-cost") <= parseInt(coins) && $(this).attr("data-state") == 'marked';
                    else
                        return $(this).attr("data-cost") <= parseInt(coins);
                }).show();
            }
        }

        $(function () {
            $("#DS, #Wii, #WiiU, #Physical, #Marked").change(function () { console.log("click"); refilter(); });
            $("#my_coins").keyup(function () { console.log("type"); refilter(); });

            $(games).each(function (i) {
                var es = "";
                if (this.eshop != null) es = '<div style="padding-top:30px;"><div style="float:left;">'
                    + '<a href="' + this.eshoplink + '" target="_blank"><img src="/Content/Images/CN/eshop.png" height="20px"/></a></div><div style="float:left;padding-top: 0px;font-size: 18px;padding-left: 20px;">' + this.eshop + '</div></div>';

                var ap = '<li class="row game-item" data-cost="' + this.price + '" data-system="'
                    + this.system + '"><div class="col-xs-4 col-sm-4 col-md-3 col-lg-3 col-xl-3"><a target="_blank" href="' + this.cnlink + '">'
                    + '<img src="' + this.url + '" class="img-responsive game-banner" /></a></div>'
                    + '<div class="col-xs-4 col-sm-4 col-md-5 col-lg-4 col-xl-4"><span class="' + this.selector + '"></span>'
                    + '<span class="game-coin"><img src="/Content/Images/CN/coin.png" /><span >' + this.price + ' coins</span></span>'
                    + '<div class="game-title">' + this.name + '</div>'
                    + es
                    + '<div style="clear:both; float:left; padding-top: 5px; cursor: pointer;" class="star-mark" data-state="off" data-id="' + i + '" data-price="'+ this.price +'">'
                    + '<img src="/Content/Images/CN/star-off.png" alt="Mark/Umark this item"> <span style="font-size: 10px; color: gray;">Mark</span></div>'
                    + '</div></li>';
                $(".game-list").append(ap);

                $(".marked-list-parent").append('<li id="ml-id' + i + '" style="clear:both; display:none;"><div class="marked-list"><div class="mark-game-price">' + this.price + '</div>'
                    + '<div class="mark-game-name">' + this.name + '</div></div></li>');
            });

            $(".star-mark").click(function () {

                if ($(this).attr("data-state") == "off") {
                    // add
                    $(this).find("img").attr("src", "/Content/Images/CN/star.png");
                    $(this).attr("data-state", "on");
                    $(this).parent().parent().css('background-color', '#FFFFD6').attr("data-state", "marked");

                    var tid = $(this).attr("data-id");
                    var price = parseInt($(this).attr("data-price"));
                    ctotal += price;
                    $("#ml-id" + tid).show();
                }
                else {
                    // remove
                    $(this).find("img").attr("src", "/Content/Images/CN/star-off.png");
                    $(this).attr("data-state", "off");
                    $(this).parent().parent().css('background-color', 'white').attr("data-state", "unmarked");
                    var tid = $(this).attr("data-id");
                    $("#ml-id" + tid).hide();
                    var price = parseInt($(this).attr("data-price"));
                    ctotal -= price;
                }

                $(".total-price").html(ctotal);
                refilter();
            });

            //$("div:gt(718)").each(function () { $(this).attr("style", "display: none;"); });
            //console.log($("div").length);
        });

